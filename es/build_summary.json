{
  "query": {
    "filtered": {
      "query": {
        "query_string": {
          "analyze_wildcard": true,
          "query": "*"
        }
      },
      "filter": {
        "bool": {
          "must": [
            {
              "range": {
                "@timestamp": {
                  "gte": "now-1w"
                }
              }
            }
          ],
          "must_not": []
        }
      }
    }
  },
  "size": 0,
  "aggs": {
    "by_package": {
      "terms": {
        "field": "main.name.raw",
        "size": 15,
        "order": {
          "_count": "desc"
        }
      },
      "aggs": {
        "by_commit": {
          "terms": {
            "field": "main.hash.raw",
            "size": 50,
            "order": {
              "_count": "desc"
            }
          },
          "aggs": {
            "build_id": {
              "terms": {
                "field": "data.id",
                "size": 50,
                "order": {
                  "_term": "desc"
                }
              },
              "aggs": {
                "architecture": {
                  "terms": {
                    "field": "data.buildVariables.ARCHITECTURE.raw",
                    "size": 5,
                    "order": {
                      "_count": "desc"
                    }
                  },
                  "aggs": {
                    "alidist_repo": {
                      "terms": {
                        "field": "data.buildVariables.ALIDIST_REPO.raw",
                        "size": 5,
                        "order": {
                          "_count": "desc"
                        }
                      },
                      "aggs": {
                        "alidist_branch": {
                          "terms": {
                            "field": "data.buildVariables.ALIDIST_BRANCH.raw",
                            "size": 5,
                            "order": {
                              "_count": "desc"
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
